{
  "name": "emacs-isearch-mark",
  "version": "1.0.0",
  "description": "Emacs keybindings with Isearch and mark set.",
  "main": "index.js",
  "scripts": {
    "dev": "esbuild src/main.ts --bundle --platform=node --external:obsidian --external:@codemirror/* --external:@lezer/* --outfile=main.js --format=cjs --watch",
    "build": "tsc --noEmit && esbuild src/main.ts --bundle --platform=node --external:obsidian --external:@codemirror/* --external:@lezer/* --outfile=main.js --format=cjs",
    "lint": "eslint src --ext .ts",
    "fix": "prettier --write \"src/**/*.ts\" \"*.{json,md,css}\" && eslint src --ext .ts --fix"
  },
  "keywords": [
    "obsidian",
    "obsidian-plugin",
    "emacs",
    "keybindings"
  ],
  "author": "myaccount-hash",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/myaccount-hash/obsidian-emacs"
  },
  "type": "commonjs",
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "esbuild": "^0.27.2",
    "eslint": "^9.39.2",
    "eslint-plugin-obsidianmd": "^0.1.9",
    "obsidian": "^1.11.0",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3"
  }
}
